{
  "expo": {
    "name": "Social Code",
    "slug": "social-code-mobile",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/SCLogo.png",
    "splash": {
      "image": "./assets/SCLogo_GradientBackground.png",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.socialcode.socialcode",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to take pictures of study group documents",
        "NSPhotoLibraryUsageDescription": "This app uses the photo library to upload study group images"
      }
    },
    "android": {
      "package": "com.socialcode.socialcode"
    },
    "description": ""
  }
}